# Exit menu to include
# include this in your menu by adding:
# [include] (~/.fluxbox/exitmenu)
#
[submenu] (exit)
      [exec] (lock) {i3exit lock}
      [exit] (exit)
      [exec] (suspend) {i3exit suspend}
      [exec] (hibernate) {i3exit hibernate}
      [exec] (reboot) {i3exit reboot}
      [exec] (shutdown) {i3exit shutdown}
[end]
